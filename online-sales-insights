   "source": [
    "# Extrair o ano e mês da coluna 'Date'\n",
    "df['date'] = pd.to_datetime(df['date'])\n",
    "df['month'] = df['date'].dt.strftime('%Y-%m')  # Extraindo o ano e mês da data\n",
    "df_grouped_units = df.groupby('month')['units_sold'].sum().reset_index()\n",
    "\n",
    "# Criando o histograma com plotly express\n",
    "fig = px.bar(df_grouped_units, x='month', y='units_sold', labels={'month': 'Month', 'units_sold': 'Units Sold'},\n",
    "             title='Units Sold by Month <br><sup>March has the most units sold</sup>',\n",
    "             color=['red' if date == '2024-03' else 'blue' for date in df_grouped_units.month], text='units_sold')\n",
    "\n",
    "fig.update_traces(textposition='outside', showlegend=False)\n",
    "\n",
    "fig.update_layout(width=1080, bargap=0.2)\n",
    "\n",
    "# Exibindo o gráfico\n",
    "fig.show()"
