    "# Extrair o ano e mês da coluna 'Date'\n",
    "df['date'] = pd.to_datetime(df['date'])\n",
    "df['month'] = df['date'].dt.strftime('%Y-%m')  # Extraindo o ano e mês da data\n",
    "df_grouped = df.groupby('month')['units_sold'].sum().reset_index()\n",
    "\n",
    "# Criando o histograma com plotly express\n",
    "fig = px.bar(df_grouped, x='month', y='units_sold', labels={'month': 'Month', 'units_sold': 'Units Sold'},\n",
    "             title='Units Sold by Month', color=['red' if date == '2024-03' else 'blue' for date in df_grouped.month], text='units_sold')\n",
