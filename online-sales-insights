   "source": [
    "def transform_columns_to_snake_case(df):\n",
    "    def snake_case(text):\n",
    "        text = text.strip().lower()\n",
    "        text = re.sub(r'[\\s\\-\\/]+', '_', text)\n",
    "        return text\n",
    "    \n",
    "    df.columns = [snake_case(col) for col in df.columns]\n",
    "    \n",
    "    return df\n",
    "\n",
    "transform_columns_to_snake_case(df)\n",
    "df.head()"
   ]
  },
