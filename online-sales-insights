    "             labels={'month': 'Month', 'total_revenue': 'Total Revenue'},\n",
    "             title='Electronic Products Revenue by Month',\n",
    "             color=['red' if date == '2024-01' else 'blue' for date in df_grouped.month], \n",
    "             text='total_revenue', \n",
    "             color_discrete_map={\"red\": \"#EF553B\", \"blue\": \"#636EFA\"})\n",
    "\n",
    "fig.update_traces(texttemplate='%{text:.2s}', textposition='outside', showlegend=False)\n",
    "\n",
    "fig.update_layout(width=1080, bargap=0.2)\n",
    "\n",
    "# Exibindo o gráfico\n",
    "fig.show()"
   ]
